    "cron": "^1.3.0",
    "cross-env": "^5.1.3",
    "eslint": "^4.15.0",
    "express": "^4.16.2",
    "helmet": "^3.9.0",
    "jsonwebtoken": "^8.1.0",
    "lodash": "^4.17.4",
    "moment": "^2.20.1",
    "mongoose": "^5.0.0",
    "morgan": "^1.9.0",
    "swagger-express-mw": "^0.7.0",
    "winston": "^2.4.0"
